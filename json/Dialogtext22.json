[
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3299.mp3",
        "de_word": "Koffer packen",
        "hr_word": "Pakirati kov\u010deg",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3301.mp3",
        "de_word": "Petar freut sich. Bald besucht er seinen Bruder.",
        "hr_word": "Petar se veseli. Uskoro \u0107e posjetiti svog brata.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3302.mp3",
        "de_word": "Er f\u00e4ngt an seine Sachen zu packen.",
        "hr_word": "Po\u010dinje pakirati svoje stvari.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3303.mp3",
        "de_word": "Sein Koffer ist schwerer, als er im letzten Urlaub war: Er m\u00f6chte viel mehr mitnehmen.",
        "hr_word": "Kov\u010deg mu je te\u017ei nego \u0161to je bio na zadnjem odmoru: \u017eeli ponijeti puno vi\u0161e stvari.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3304.mp3",
        "de_word": "Er braucht auch noch Geschenke f\u00fcr seinen Bruder und den Gro\u00dfvater.",
        "hr_word": "Treba jo\u0161 i poklone za svog brata i djeda.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3305.mp3",
        "de_word": "Er ruft Ana an, um sie um Rat zu bitten.",
        "hr_word": "Zove Anu kako bi je pitao za savjet.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3306.mp3",
        "de_word": "Hallo Ana,",
        "hr_word": "Bok Ana,",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3307.mp3",
        "de_word": "Ich wei\u00df nicht, was ich meinem Bruder und meinem Gro\u00dfvater schenken soll.",
        "hr_word": "Ne znam \u0161to da poklonim svom bratu i djedu.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3308.mp3",
        "de_word": "Heute ist doch Flohmarkt. Da wirst du sicher einige sch\u00f6ne Sachen finden.",
        "hr_word": "Danas je pijaca. Tamo \u0107e\u0161 sigurno na\u0107i neke lijepe stvari.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3309.mp3",
        "de_word": "Ich hatte eigentlich an ein Buch oder eine CD gedacht.",
        "hr_word": "Mislio sam mo\u017eda neku knjigu ili CD.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3310.mp3",
        "de_word": "Aber ich k\u00f6nnte mich trotzdem mal auf dem Flohmarkt umsehen.",
        "hr_word": "Ali mogao bih svejedno pogledati na buvljaku.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3311.mp3",
        "de_word": "Daneben gibt es einen Zeitungskiosk. Bei dem wollte ich mir sowieso f\u00fcr die Fahrt Zeitschriften und eine Zeitung besorgen.",
        "hr_word": "Pored ima kiosk s novinama. Tamo sam tako i tako htio nabaviti \u010dasopise i novine za vo\u017enju.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3312.mp3",
        "de_word": "Ins Einkaufszentrum muss ich auch noch.",
        "hr_word": "Moram jo\u0161 i u shopping-centar.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3313.mp3",
        "de_word": "Ich brauche noch viele Sachen.",
        "hr_word": "Trebam jo\u0161 puno stvari.",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3314.mp3",
        "de_word": "Was fehlt dir denn?",
        "hr_word": "\u0160to ti nedostaje?",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3315.mp3",
        "de_word": "Auf meiner Liste steht:",
        "hr_word": "Na popisu stoji:",
        "category": "Dialogtext22"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3316.mp3",
        "de_word": "Deodorant, Duschgel, Kamm, Seife, Zahnb\u00fcrste.",
        "hr_word": "Dezodorans, gel za tu\u0161iranje, \u010de\u0161alj, sapun, \u010detkica za zube.",
        "category": "Dialogtext22"
    }
]