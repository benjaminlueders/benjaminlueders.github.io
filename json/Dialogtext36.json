[
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3546.mp3",
        "de_word": "Neue Skier bestellen",
        "hr_word": "Naru\u010diti nove skije",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3548.mp3",
        "de_word": "Petar und Ana sitzen vor dem Computer und suchen Skier im Internet.",
        "hr_word": "Petar i Ana sjede pred ra\u010dunalom i na internetu tra\u017ee nove skije.",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3549.mp3",
        "de_word": "Ana wei\u00df genau, welche Skier sie m\u00f6chte.",
        "hr_word": "Ana to\u010dno zna koje skije \u017eeli.",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3550.mp3",
        "de_word": "Ich konnte sie nirgendwo finden.",
        "hr_word": "Nisam ih nigdje mogla na\u0107i.",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3551.mp3",
        "de_word": "Ich war in jedem Sportgesch\u00e4ft in dieser Stadt.",
        "hr_word": "Bila sam u svakom sportskom du\u0107anu u ovom gradu.",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3552.mp3",
        "de_word": "Daher muss ich sie im Internet bestellen.",
        "hr_word": "Zato ih moram naru\u010diti preko interneta.",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3553.mp3",
        "de_word": "Aber warum brauchst du gerade diese speziellen Skier?",
        "hr_word": "Ali za\u0161to ti trebaju ba\u0161 te posebne skije?",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3554.mp3",
        "de_word": "Weil sie von guter Qualit\u00e4t und trotzdem g\u00fcnstig sind.",
        "hr_word": "Zato \u0161to su dobre kvalitete a unato\u010d tome su povoljne.",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3555.mp3",
        "de_word": "Wenn du sie \u00fcber das Internet bestellst, bekommst du sie dann per Post zugesandt?",
        "hr_word": "Kad ih naru\u010di\u0161 preko interneta, dobije\u0161 li ih po\u0161tom?",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3556.mp3",
        "de_word": "Oder musst du sie dann irgendwo abholen?",
        "hr_word": "Ili ih mora\u0161 negdje podi\u0107i?",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3557.mp3",
        "de_word": "Ich bekomme sie direkt nach Hause geliefert.",
        "hr_word": "Dobijem ih direktno dostavljene doma.",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3558.mp3",
        "de_word": "Soll ich sie in wei\u00df, gelb, gr\u00fcn oder in braun nehmen?",
        "hr_word": "Da uzmem bijele, \u017eute, zelene ili sme\u0111e?",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3559.mp3",
        "de_word": "Vielleicht in gr\u00fcn?",
        "hr_word": "Mo\u017eda zelene?",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3560.mp3",
        "de_word": "Nein! Ich wei\u00df, in welcher Farbe ich sie m\u00f6chte! Rate!",
        "hr_word": "Ne! Znam koju boju \u017eelim! Poga\u0111aj!",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3561.mp3",
        "de_word": "Lass mich nachdenken!",
        "hr_word": "Da razmislim!",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3562.mp3",
        "de_word": "Du nimmst sie in gelb.",
        "hr_word": "Uzet \u0107e\u0161 \u017eute.",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3563.mp3",
        "de_word": "Richtig!",
        "hr_word": "To\u010dno!",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3564.mp3",
        "de_word": "Jetzt muss ich nur noch das Online-Formular ausf\u00fcllen.",
        "hr_word": "Sad moram jo\u0161 samo ispuniti online-formular.",
        "category": "Dialogtext36"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3566.mp3",
        "de_word": "Ich zahle per Rechnung.",
        "hr_word": "Pla\u0107am ra\u010dunom.",
        "category": "Dialogtext36"
    }
]