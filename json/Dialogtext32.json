[
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3470.mp3",
        "de_word": "Der Unfall",
        "hr_word": "Nesre\u0107a",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3472.mp3",
        "de_word": "Auf dem Weg zur Geburtstagsparty h\u00f6ren sie jemanden um Hilfe rufen.",
        "hr_word": "Na putu na ro\u0111endansku fe\u0161tu \u010duju kako netko zove u pomo\u0107.",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3473.mp3",
        "de_word": "Beide rennen in die Richtung, aus der man die Stimme h\u00f6rt.",
        "hr_word": "Oboje tr\u010de u smjeru iz kojeg se \u010duje glas .",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3474.mp3",
        "de_word": "Ein alter Mann sitzt ganz allein am Rand der Stra\u00dfe.",
        "hr_word": "Neki stari \u010dovjek sjedi posve sam na rubu ulice.",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3475.mp3",
        "de_word": "Was ist passiert?",
        "hr_word": "\u0160to se dogodilo?",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3476.mp3",
        "de_word": "Alles geschah so schnell.",
        "hr_word": "Sve se dogodilo tako brzo.",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3477.mp3",
        "de_word": "Erz\u00e4hlen Sie uns die ganze Geschichte!",
        "hr_word": "Ispri\u010dajte nam cijelu pri\u010du!",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3478.mp3",
        "de_word": "Meine Nachbarn feierten Hochzeit und dann fing etwas Feuer.",
        "hr_word": "Moji susjedi su slavili vjen\u010danje i onda je po\u010deo po\u017ear.",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3479.mp3",
        "de_word": "Die Flammen gerieten au\u00dfer Kontrolle.",
        "hr_word": "Plamen je izmakao kontroli.",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3480.mp3",
        "de_word": "Sie verteilten sich im ganzen Haus, im Wohnzimmer, im Erdgeschoss, \u00fcberall.",
        "hr_word": "Razbuktao se po cijeloj ku\u0107i, u dnevnoj sobi, u prizemlju, svuda.",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3481.mp3",
        "de_word": "Jeder rannte und schrie und schlug die Fenster ein.",
        "hr_word": "Svi su tr\u010dali i vikali i razbijali prozore.",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3482.mp3",
        "de_word": "Und dann war da dieser tapfere Feuerwehrmann.",
        "hr_word": "A onda je do\u0161ao taj hrabri vatrogasac.",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3483.mp3",
        "de_word": "Er half jedem heraus. Die Leute, die sich verletzt haben, sind jetzt im Krankenhaus.",
        "hr_word": "Svakome je pomogao iza\u0107i. Ljudi koji su se ozlijedili su sada u bolnici.",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3484.mp3",
        "de_word": "Und Sie? Sind Sie verletzt?",
        "hr_word": "A Vi? Jeste li Vi ozlije\u0111eni?",
        "category": "Dialogtext32"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3485.mp3",
        "de_word": "Nein, ich bin unverletzt. Zum Gl\u00fcck ist mir nichts passiert.",
        "hr_word": "Ne, ja sam neozlije\u0111en. Na sre\u0107u mi se ni\u0161ta nije dogodilo.",
        "category": "Dialogtext32"
    }
]