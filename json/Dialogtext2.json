[
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3016.mp3",
        "de_word": "Im Hotel",
        "hr_word": "U hotelu",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3018.mp3",
        "de_word": "Petar sucht ein Zimmer.",
        "hr_word": "Petar tra\u017ei sobu.",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3019.mp3",
        "de_word": "Er spricht mit der Dame an der Rezeption eines Hotels:",
        "hr_word": "Razgovara sa gospo\u0111om na recepciji jednog hotela:",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3020.mp3",
        "de_word": "Guten Abend. Ich suche ein sch\u00f6nes Einzelzimmer.",
        "hr_word": "Dobra ve\u010der. Tra\u017eim lijepu jednokrevetnu sobu.",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3021.mp3",
        "de_word": "Kein Problem.",
        "hr_word": "Nema problema.",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3022.mp3",
        "de_word": "Wir haben oben gro\u00dfe und helle Zimmer mit Bad. Ist das in Ordnung?",
        "hr_word": "Gore imamo velike i svijetle sobe s kupaonicom. Je li to u redu?",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3023.mp3",
        "de_word": "Selbstverst\u00e4ndlich, das ist gut.",
        "hr_word": "Naravno, to je dobro.",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3024.mp3",
        "de_word": "Sagen Sie, ist das Zimmer mit einem Fernseher ausgestattet?",
        "hr_word": "Recite, je li soba opremljena s televizorom?",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3025.mp3",
        "de_word": "Ja, wir haben sogar Satellitenfernsehen.",
        "hr_word": "Da, imamo \u010dak i satelitski program.",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3026.mp3",
        "de_word": "Sie k\u00f6nnen sechzig Fernsehsender sehen.",
        "hr_word": "Mo\u017eete gledati \u0161ezdeset programa.",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3027.mp3",
        "de_word": "In jedem Zimmer gibt es ein Radio.",
        "hr_word": "U svakoj sobi ima radio.",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3028.mp3",
        "de_word": "Wunderbar. Ich nehme eines der Zimmer.",
        "hr_word": "Odli\u010dno. Uzet \u0107u jednu od tih soba.",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3029.mp3",
        "de_word": "K\u00f6nnen Sie mir bitte den Schl\u00fcssel geben?",
        "hr_word": "Mo\u017eete li mi, molim Vas, dati klju\u010d?",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3030.mp3",
        "de_word": "Gerne, hier ist er.",
        "hr_word": "Rado, evo ga.",
        "category": "Dialogtext2"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3031.mp3",
        "de_word": "Fr\u00fchst\u00fcck gibt es von 7.30 Uhr bis 10 Uhr.",
        "hr_word": "Doru\u010dak je od pola osam do deset sati.",
        "category": "Dialogtext2"
    }
]