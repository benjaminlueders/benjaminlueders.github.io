[
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3186.mp3",
        "de_word": "Einkaufsliste",
        "hr_word": "Popis za kupovinu",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3188.mp3",
        "de_word": "Petar m\u00f6chte heute Abend ein Fest f\u00fcr seine Freunde machen, weil er wieder nach Hause zur\u00fcck gekommen ist.",
        "hr_word": "Petar \u017eeli ve\u010deras prirediti zabavu za svoje prijatelje, jer se opet vratio ku\u0107i.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3189.mp3",
        "de_word": "Er ist ein sehr guter Koch und er l\u00e4dt seine Freunde gerne ein.",
        "hr_word": "Vrlo je dobar kuhar i rado poziva svoje prijatelje.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3190.mp3",
        "de_word": "Doch heute ist sein K\u00fchlschrank leer.",
        "hr_word": "No danas mu je fri\u017eider prazan.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3191.mp3",
        "de_word": "Er muss dringend einkaufen gehen.",
        "hr_word": "Mora hitno i\u0107i u kupovinu.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3192.mp3",
        "de_word": "Er wird griechische Spezialit\u00e4ten kochen.",
        "hr_word": "Kuhat \u0107e gr\u010dke specijalitete.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3193.mp3",
        "de_word": "Er notiert eine Liste:",
        "hr_word": "Radi popis:",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3194.mp3",
        "de_word": "F\u00fcr den griechischen Salat: Essig, Oliven\u00f6l, Salat, Oliven, K\u00e4se.",
        "hr_word": "Za gr\u010dku salatu: ocat, maslinovo ulje, salata, masline, sir.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3195.mp3",
        "de_word": "F\u00fcr die Fischsuppe: Garnelen, Lachs, Muscheln, Thunfisch, Kr\u00e4uter, Reis.",
        "hr_word": "Za riblju juhu: ra\u010di\u0107i, losos, \u0161koljke, tuna, za\u010dinsko bilje, ri\u017ea.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3196.mp3",
        "de_word": "F\u00fcr das Hauptgericht: Lamm, Rindfleisch, Kartoffeln, frisches Gem\u00fcse.",
        "hr_word": "Za glavno jelo: janjetina, govedina, krumpiri, svje\u017ee povr\u0107e.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3197.mp3",
        "de_word": "F\u00fcr den Nachtisch: Schokolade, Schlagsahne, Eiscreme.",
        "hr_word": "Za desert: \u010dokolada, \u0161lag, sladoled.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3198.mp3",
        "de_word": "Getr\u00e4nke: Apfelsaft, Bier und Rotwein.",
        "hr_word": "Pi\u0107a: sok od jabuke, pivo i crno vino.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3199.mp3",
        "de_word": "Er denkt:",
        "hr_word": "Misli:",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3200.mp3",
        "de_word": "Dies ist eine lange Liste.",
        "hr_word": "Duga\u010dak je to popis.",
        "category": "Dialogtext15"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3201.mp3",
        "de_word": "Das muss genug sein f\u00fcr heute Abend.",
        "hr_word": "To mora biti dosta za ve\u010deras.",
        "category": "Dialogtext15"
    }
]