[
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3429.mp3",
        "de_word": "Essen kochen",
        "hr_word": "Kuhati jelo",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3431.mp3",
        "de_word": "Petar und Ana haben die Eink\u00e4ufe erledigt.",
        "hr_word": "Petar i Ana su obavili kupovinu.",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3432.mp3",
        "de_word": "Sie entscheiden sich, zu Abend zu essen.",
        "hr_word": "Odlu\u010duju se ve\u010derati.",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3433.mp3",
        "de_word": "Was wollen wir kochen?",
        "hr_word": "\u0160to \u017eelimo kuhati?",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3434.mp3",
        "de_word": "Ich m\u00f6chte ein Steak.",
        "hr_word": "Ja bi htio odrezak.",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3435.mp3",
        "de_word": "Magst du es gut durchgebraten?",
        "hr_word": "Voli\u0161 li jako pe\u010den?",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3436.mp3",
        "de_word": "Nein, ich mag es medium, mit vielen Gew\u00fcrzen und Senf.",
        "hr_word": "Ne, volim srednje pe\u010den, s puno za\u010dina i senfa.",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3437.mp3",
        "de_word": "Ich w\u00fcrze es immer nur mit Salz und Pfeffer.",
        "hr_word": "Ja ga uvijek za\u010dinim samo sa soli i paprom.",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3438.mp3",
        "de_word": "Gut. Dann br\u00e4t sich eben jeder sein Steak selbst.",
        "hr_word": "Dobro. Onda \u0107e si svatko pe\u0107i svoj odrezak.",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3439.mp3",
        "de_word": "Dazu kann ich Reis kochen.",
        "hr_word": "Uz to mogu skuhati ri\u017eu.",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3440.mp3",
        "de_word": "Brauchen wir daf\u00fcr noch etwas?",
        "hr_word": "Trebamo li uz to jo\u0161 ne\u0161to?",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3441.mp3",
        "de_word": "Wir m\u00fcssen noch zur Metzgerei gehen.",
        "hr_word": "Moramo jo\u0161 i\u0107i u mesnicu.",
        "category": "Dialogtext30"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3442.mp3",
        "de_word": "Falls sie dort keine Steaks mehr haben, nehmen wir Gefl\u00fcgel.",
        "hr_word": "Ako tamo nemaju odrezaka, uzet \u0107emo perad.",
        "category": "Dialogtext30"
    }
]