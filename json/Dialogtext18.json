[
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3227.mp3",
        "de_word": "Der Ausflug",
        "hr_word": "Izlet",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3229.mp3",
        "de_word": "Ana ist in Rijeka geboren und aufgewachsen. Sie l\u00e4dt Petar zu einem Ausflug dorthin ein.",
        "hr_word": "Ana je ro\u0111ena i odrasla u Rijeci. Poziva Petra na izlet tamo.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3230.mp3",
        "de_word": "Sie wird ihm ihre Stadt zeigen.",
        "hr_word": "Pokazat \u0107e mu svoj grad.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3231.mp3",
        "de_word": "Am Samstag Morgen fahren sie los. Nach zwei Stunden kommen sie an.",
        "hr_word": "Kre\u0107u u subotu ujutro. Sti\u017eu nakon dva sata.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3232.mp3",
        "de_word": "Zusammen gehen sie durch die Altstadt.",
        "hr_word": "Zajedno idu kroz stari grad.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3233.mp3",
        "de_word": "Dies ist der Ort, wo ich die meiste Zeit verbracht habe, als ich j\u00fcnger war.",
        "hr_word": "Ovo je mjesto na kojem sam provodila najvi\u0161e vremena dok sam bila mlada.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3234.mp3",
        "de_word": "In einer Kunstgalerie?",
        "hr_word": "U umjetni\u010dkoj galeriji?",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3235.mp3",
        "de_word": "Ja, sie geh\u00f6rte meiner Tante.",
        "hr_word": "Da, pripada mojoj tetki.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3236.mp3",
        "de_word": "Sie sa\u00df dort immer und schrieb Geschichten \u00fcber die K\u00fcnstler.",
        "hr_word": "Uvijek je sjedila tamo i pisala pri\u010de o umjetnicima.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3237.mp3",
        "de_word": "Meine Mama f\u00fchrte darin die Gruppen herum.",
        "hr_word": "Moja mama je vodila grupe okolo.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3238.mp3",
        "de_word": "Einmal wurde hier ein Film \u00fcber einen der K\u00fcnstler gedreht.",
        "hr_word": "Jednom je ovdje sniman film o jednom umjetniku.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3239.mp3",
        "de_word": "Ich habe sogar in meinem Reisef\u00fchrer eine Notiz dar\u00fcber gelesen.",
        "hr_word": "O tome sam \u010dak pro\u010ditao u mom vodi\u010du.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3240.mp3",
        "de_word": "Deshalb sind so viele Touristen hier!",
        "hr_word": "Zato je ovdje toliko turista!",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3241.mp3",
        "de_word": "Ja, es ist ganz sch\u00f6n voll hier. Wollen wir jetzt woanders hin?",
        "hr_word": "Da, ovdje je prili\u010dno puno. Ho\u0107emo li sada i\u0107i negdje drugdje?",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3242.mp3",
        "de_word": "Die beiden gehen weiter.",
        "hr_word": "Idu dalje.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3243.mp3",
        "de_word": "Was ist das dort f\u00fcr ein gro\u00dfes Geb\u00e4ude neben der Bank?",
        "hr_word": "\u0160to je ona velika zgrada tamo pored banke?",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3244.mp3",
        "de_word": "Oh, das war fr\u00fcher einmal ein Gef\u00e4ngnis.",
        "hr_word": "O, to je neko\u0107 bio zatvor.",
        "category": "Dialogtext18"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3245.mp3",
        "de_word": "Heute ist das ein Museum. Die oberen Stockwerke werden an Rechtsanw\u00e4lte, \u00c4rzte und Zahn\u00e4rzte vermietet.",
        "hr_word": "Danas je to muzej. Gornji katovi se iznajmljuju odvjetnicima, lije\u010dnicima i zubarima.",
        "category": "Dialogtext18"
    }
]