[
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3360.mp3",
        "de_word": "Der Traum",
        "hr_word": "San",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3362.mp3",
        "de_word": "Nachdem Petar den Ring abgeholt hat, verlie\u00df er das Gesch\u00e4ft.",
        "hr_word": "Nakon \u0161to je Petar uzeo prsten, oti\u0161ao je iz du\u0107ana.",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3363.mp3",
        "de_word": "Er ging zum Bushaltestelle, stieg in den Bus ein und schloss seine Augen.",
        "hr_word": "Oti\u0161ao je do autobusne stanice, u\u0161ao u bus i zatvorio o\u010di.",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3364.mp3",
        "de_word": "Bald schlief er ein.",
        "hr_word": "Uskoro je zaspao.",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3365.mp3",
        "de_word": "Jemand n\u00e4herte sich ihm.",
        "hr_word": "Netko mu se pribli\u017eio.",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3366.mp3",
        "de_word": "Diese Person stahl den Ring von Anas Gro\u00dfmutter.",
        "hr_word": "Ta osoba je ukrala prsten Anine bake.",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3367.mp3",
        "de_word": "Petar schrie sofort: Geben Sie den Ring zur\u00fcck oder ich rufe die Polizei!",
        "hr_word": "Petar je odmah uzviknuo: Vra\u0107ajte prsten nazad ili zovem policiju!",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3368.mp3",
        "de_word": "Aber der Dieb verschwand.",
        "hr_word": "No lopov je nestao.",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3369.mp3",
        "de_word": "Petar bekam Panik.",
        "hr_word": "Petar se uspani\u010dio.",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3370.mp3",
        "de_word": "Er musste Ana anrufen und ihr erkl\u00e4ren, was passiert war.",
        "hr_word": "Morao je nazvati Anu i objasniti joj \u0161to se dogodilo.",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3371.mp3",
        "de_word": "Er schaute in seinem Adressbuch nach, um ihre Telefonnummer zu finden.",
        "hr_word": "Pogledao je u telefonski imenik kako bi na\u0161ao njen broj.",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3372.mp3",
        "de_word": "Der Busfahrer sagte: Endstation, bitte alle aussteigen.",
        "hr_word": "Voza\u010d autobusa je rekao: zadnja stanica, molim iza\u0111ite.",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3373.mp3",
        "de_word": "Petar war verwirrt, aber dann verstand er:",
        "hr_word": "Petar je bio zbunjen, ali onda je shvatio:",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3374.mp3",
        "de_word": "Ich habe das alles nur getr\u00e4umt!",
        "hr_word": "To sam sve samo sanjao!",
        "category": "Dialogtext26"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3375.mp3",
        "de_word": "Er schaute in seine Tasche. Der Ring war noch da.",
        "hr_word": "Pogledao je u d\u017eepove. Prsten je jo\u0161 bio tamo.",
        "category": "Dialogtext26"
    }
]