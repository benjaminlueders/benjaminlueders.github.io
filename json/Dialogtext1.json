[
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3001.mp3",
        "de_word": "Im Urlaub",
        "hr_word": "Na odmoru",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3003.mp3",
        "de_word": "Petar ist im Urlaub. Er trifft Ana.",
        "hr_word": "Petar je na odmoru. Sre\u0107e Anu.",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3004.mp3",
        "de_word": "Hallo! Ich hei\u00dfe Petar.",
        "hr_word": "Bok! Ja sam Petar.",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3005.mp3",
        "de_word": "Ich bin sechsundzwanzig Jahre alt.",
        "hr_word": "Imam dvadeset \u0161est godina.",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3006.mp3",
        "de_word": "Nett, dich kennen zu lernen. Ich hei\u00dfe Ana.",
        "hr_word": "Drago mi je. Ja sam Ana.",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3007.mp3",
        "de_word": "Woher kommst du?",
        "hr_word": "Odakle si?",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3008.mp3",
        "de_word": "Aus Kroatien. Ich wurde in Zagreb geboren.",
        "hr_word": "Iz Hrvatske. Ro\u0111en sam u Zagrebu.",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3009.mp3",
        "de_word": "Willkommen! Bist du hier im Urlaub?",
        "hr_word": "Dobrodo\u0161ao! Ovdje si na odmoru?",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3010.mp3",
        "de_word": "Ja. Lebst du hier?",
        "hr_word": "Da. Ti \u017eivi\u0161 ovdje?",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3011.mp3",
        "de_word": "Nein, ich komme auch aus Kroatien.",
        "hr_word": "Ne, i ja sam iz Hrvatske.",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3012.mp3",
        "de_word": "Ich verbringe auch meine Ferien hier.",
        "hr_word": "I ja ovdje provodim praznike.",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3013.mp3",
        "de_word": "Dann sehen wir uns nochmal. Tsch\u00fcss.",
        "hr_word": "Onda se vidimo. Bok!",
        "category": "Dialogtext1"
    },
    {
        "pic_url": "",
        "de_audio_url": "",
        "hr_audio_url": "audio/24/dialogtext_hr_audio/3014.mp3",
        "de_word": "Tsch\u00fcss. Bis bald.",
        "hr_word": "Bok! Do skora.",
        "category": "Dialogtext1"
    }
]